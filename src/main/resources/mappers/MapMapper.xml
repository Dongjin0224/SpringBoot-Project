<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.test.mappers.MapMapper">
    <select id="getSearchList" parameterType="string" resultType="com.example.test.user.vo.DocVO">
        SELECT * FROM TABLE_DOC WHERE docHospitalName LIKE '%'||#{search}||'%' OR docAddress LIKE '%'||#{search}||'%'
    </select>

    <select id="getList" resultType="com.example.test.user.vo.DocVO">
        SELECT * FROM TABLE_DOC
    </select>
</mapper>